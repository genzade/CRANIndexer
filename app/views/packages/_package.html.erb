<div id="<%= dom_id package %>">
  <p class="my-5">
    <strong class="block font-medium mb-1">Name:</strong>
    <%= package.name %>
  </p>

  <p class="my-5">
    <strong class="block font-medium mb-1">Version:</strong>
    <%= package.version %>
  </p>

  <p class="my-5">
    <strong class="block font-medium mb-1">Required r version:</strong>
    <%= package.required_r_version %>
  </p>

  <p class="my-5">
    <strong class="block font-medium mb-1">Dependencies:</strong>
    <%= package.dependencies %>
  </p>

  <p class="my-5">
    <strong class="block font-medium mb-1">Date publication at:</strong>
    <%= package.date_publication_at %>
  </p>

  <p class="my-5">
    <strong class="block font-medium mb-1">Title:</strong>
    <%= package.title %>
  </p>

  <p class="my-5">
    <strong class="block font-medium mb-1">Authors:</strong>
    <%= package.authors %>
  </p>

  <p class="my-5">
    <strong class="block font-medium mb-1">Maintainers:</strong>
    <%= package.maintainers %>
  </p>

  <p class="my-5">
    <strong class="block font-medium mb-1">License:</strong>
    <%= package.license %>
  </p>
</div>
